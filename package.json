{
  "name": "warp.css",
  "version": "3.0.0",
  "description": "A CSS library for applying commonly used styles quickly.",
  "keywords": [
    "CSS",
    "CSS 3",
    "Sass",
    "scss"
  ],
  "homepage": "https://github.com/snsakib/warp.css",
  "bugs": {
    "url": "https://github.com/snsakib/warp.css/issues",
    "email": "contact@snsakib.com"
  },
  "scripts": {
    "compile": "sass scss/main.scss css/main.css",
    "watch": "sass --watch scss/main.scss css/main.css",
    "kss": "kss --config kss-config.json",
    "all": "npm run compile && npm run kss"
  },
  "main": "./css/main.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/snsakib/warp.css.git"
  },
  "author": "Syed Nazmus Sakib <contact@snsakib.com> (https://www.snsakib.com)",
  "license": "ISC",
  "devDependencies": {
    "kss": "^3.0.0-beta.25",
    "michelangelo": "^0.8.0"
  }
}
